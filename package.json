{
  "name": "testone",
  "displayName": "TestOne",
  "description": "A VS Code extension that predicts a commit message based on Git differences.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.autofillCommitMessage",
        "title": "Autofill Commit Message"
      },
      {
        "command": "extension.getCommitMessage",
        "title": "Get Commit Message"
      }
    ],
    "configuration": {
      "title": "TestOne",
      "properties": {
        "testone.backendUrl": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "The URL of the backend server for generating commit messages."
        }
      }
    }
  },
  "enabledApiProposals": ["terminalDataWriteEvent"],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}